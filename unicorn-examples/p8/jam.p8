__lua__

function _init()
    mode(240, 136)
    cls()
end

function _update()
end

local tex = {
   8,9,5,3, 5,9,15,14,    7,10,7,7, 7,7,7,5,
   6,11,0,7, 7,7,7,5,    5,12,7,0, 10,7,7,5,
   4,13,7,11,0,7,7,5,    3,14,2,3, 4,0,6,7,
   2,15,7,7, 7,7,0,5,    1,0,9,5, 5,5,5,0 }

function _draw()
   local kek = time()/1e3
   local s,c,ry,rx,rz,t,hack = math.sin(kek),math.cos(kek),0,0,0,0
   local H = 10 - kek
   if H < 0 then print("u ded",100,60,15) return end
   
   for y=0,135 do
      ry = .01+(68-y)/136
      if ry < -.01 or ry>.01 then
         if ry > 0 then hack = kek*-10 else hack = kek*10 end
         t = H/ry
         for x=0,239 do
            rx = (x-120)/240
            rz = s*rx + c        
            rx = c*rx - s
            pset(x,y,tex[((7&math.floor(rz*t+hack))<<3)+
                           (7&math.floor(rx*t+hack))+1])
         end
      end
   end
end

