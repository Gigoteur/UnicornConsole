import random


def rnd_color():
    return random.randint(1, 16)


def rnd_radius():
    return random.randint(3, 10)


def rnd_x():
    return random.randint(0, mode_width())

def rnd_y():
    return random.randint(0, mode_height())


class Rect(object):
    def __init__(self):
        self.T = 0

    def init(self):
        cls()

    def update(self):
        self.T += 1

    def draw(self):
        if self.T % 20 == 1:
            cls()
            rect(rnd_x(), rnd_y(), rnd_x(), rnd_y(), rnd_color())
            rectfill(rnd_x(), rnd_y(), rnd_x(), rnd_y(), rnd_color())


