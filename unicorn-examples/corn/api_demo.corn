class Memcpy(object):
    def __init__(self):
        self.T = 0

    def distort_orig(self, amount):
        amount = min(amount, 64)

        for iy in range(0, 127):
            a = flr(-amount + rnd(amount*2) + 0.5)

            s = max(-a, 0)
            e = max(a, 0)


            l = 64 - abs(a)
            adr = 0x0 + iy*64
            memcpy(adr + e, adr + s, l)

    def init(self):
        self.dist_amt = 2

    def update(self):
        self.T += 1
        if btn(5):
            self.dist_amt += 0.1

    def draw(self):
        cls()

        rectfill(10, 10, 120, 120, 3)
        circfill(63, 63, 16, 7)
        circfill(10, 50, 23, 10)
        circfill(50, 10, 23, 8)
        circfill(80, 80, 23, 11)
        circfill(120, 80, 23, 11)

        if btn(4):
            self.distort_orig(self.dist_amt)


class Print(object):
    def __init__(self):
        self.T = 0
        self.values = {}

    def init(self):
        pass

    def update(self):
        for i in range(32, 126):
            if i >= 65 and i <= 90:
                self.values[i] = btn(chr(i+32))
            else:
                self.values[i] = btn(chr(i))


    def draw(self):
        cls()
        x = 0
        y = 0
        for i in range(32, 126):
            if self.values.get(i):
                unicorn_print(chr(i), x, y, 7)
            else:
                unicorn_print(chr(i), x, y, 9)
            x += 8
            if x > 0 and x % 128 == 0:
                x = 0
                y += 8


class Pal(object):
    def __init__(self):
        self.T = 0
        self.COUNT = 0

    def init(self):
        palt()

    def update(self):
        if self.COUNT % 10 == 0:
            print(self.T)

            palt()
            palt(self.T, True)
            self.T = (self.T + 1) % 16
        self.COUNT += 1

    def draw(self):
        cls()
        for i in range(0, 16):
            rectfill(0, i*8, 128, i*8 + 8, i)

idx_demo = 0
demos = [
    ["rect", [Rect()]],
    ["circ", [Circ()]],
    ["trigon", [Trigon()]],
    ["ellipse", [Ellipse()]],
    ["polygon", [Polygon()]],
    ["line", [Line()]],
    ["spr", [Spr()]],
    ["sspr", [SSpr()]],
    ["camera", [Camera()]],
    ["clip", [Clip()]],
    ["colors", [Colors()]],
    ["palette", [Palette()]],
    ["map", [Map()]],
    ["memcpy", [Memcpy()]],
    ["print", [Print()]],
    ["pal", [Pal()]],
]


def _init():
    global demos
    cls()


def _update():
    global idx_demo, demos

    if btnp(0):
        palette_switch("pico-8")

        idx_demo = (idx_demo - 1) % len(demos)
        pal()
        for demo in demos[idx_demo][1]:
            demo.init()

    if btnp(1):
        palette_switch("pico-8")

        idx_demo = (idx_demo + 1) % len(demos)
        pal()
        for demo in demos[idx_demo][1]:
            demo.init()

    for demo in demos[idx_demo][1]:
        demo.update()


def _draw():
    global idx_demo, demos

    for demo in demos[idx_demo][1]:
        demo.draw()

    unicorn_print("Demos " + demos[idx_demo][0], 0, 120, 2)

