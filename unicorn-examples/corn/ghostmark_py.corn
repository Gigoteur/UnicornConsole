import random

# Original code is here: http://tic.computer/play?cart=122

class Item(object):
    def __init__(self):
        self.x = random.randrange(10, mode_width())
        self.y = random.randrange(10, mode_height())
        self.vx = random.randrange(-10,10) / 10.0
        self.vy = random.randrange(-10,10) / 10.0
        self.color = rnd(15)

    def update(self):
        self.x = self.x + self.vx
        self.y = self.y + self.vy

        if self.x<0 or self.x > mode_width():
            self.vx = -self.vx

        if self.y > mode_height():
            self.y = mode_height()
            self.vy = -(rnd(100)/25.0)


        spr(self.color, self.x, self.y)
        self.vy = self.vy + 0.05

Items = []
for i in range(0, 500):
    Items.append(Item())

def _init():
    pass

def _update():

    #    add 500 more
    unicorn_print("(X) add 500 more", 8, mode_width() - 20, 7)

    if btnp(5):
        for i in range(0, 500):
            Items.append(Item())

def _draw():
    cls()

    for item in Items:
        item.update()


    unicorn_print("dots :" + str(len(Items)), mode_width() - 60, 0, 7)


